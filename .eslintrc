{
  "extends": "airbnb-base",
  "parser": "babel-eslint",
  "rules": {
    "import/no-extraneous-dependencies": ["error", {
      "devDependencies": true,
      "optionalDependencies": false,
      "peerDependencies": false,
    }]
  },
  "plugins": [
    "babel"
  ],
  "env": {
    "browser": true
  }
}
